#include<stdio.h>
int main()
{
	int i,n,s,a[100],c=0;
	printf("enter the no of elements:");
	scanf("%d",&n);
	printf("enter the search element:");
	scanf("%d",&s);
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
	}
	for(i=0;i<n;i++){
		if(a[i]==s){
			c=i;
		}
	}
	if(c==0){
		printf("search element is not found");
	}
	else{
		printf("search element is at index %d",c);
	}
}
