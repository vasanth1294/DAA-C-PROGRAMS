#include<stdio.h>
int main()
{
	int n,i,a[100],ind,ele;
	printf("enter the no of elements:");
	scanf("%d",&n);
	printf("enter the index to insert:");
	scanf("%d",&ind);
	if(ind<=n-1){
	printf("enter the element to be inserted:");
	scanf("%d",&ele);
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
	}
	for(i=0;i<n;i++){
		a[ind-1]=ele;
	}
	for(i=0;i<n;i++){
		printf("%d\n",a[i]);
	}
	}
	else{
		printf("invalid");
	}
}
