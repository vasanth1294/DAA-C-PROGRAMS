#include<stdio.h>
int main()
{
	int i,j,k;
	int a[3];
	printf("enter the num1:");
	scanf("%d",&a[0]);
	printf("enter the num2:");
	scanf("%d",&a[1]);
	printf("enter the num3:");
	scanf("%d",&a[2]);
	for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			for(k=0;k<3;k++){
				if(i!=j && j!=k && k!=i)
				{
					printf("[%d %d %d]\n",a[i],a[j],a[k]);
				}
			}
		}
	}
}
